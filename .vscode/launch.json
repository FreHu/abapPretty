  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "list package recursively",
      "smartStep": true,
      "program": "${workspaceFolder}/bin/run",
      "args": ["list", "NPL", "DEVC/K", "$ABAPGIT", "-r"],
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "simulate FG",
      "smartStep": true,
      "program": "${workspaceFolder}/bin/run",
      "args": ["simulate", "NPL", "FUGR/F", "ZABAPGIT_PARALLEL"],
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "format include",
      "smartStep": true,
      "program": "${workspaceFolder}/bin/run",
      "args": ["prettyprint", "NPL", "PROG/I", "ZDEMO_EXCEL_OUTPUTOPT_INCL"],
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "process generated code",
      "smartStep": true,
      "program": "${workspaceFolder}/bin/run",
      "args": ["prettyprint", "NPL", "INTF/OI", "ZIWCI_DEMO_EXCEL_WDA01"],
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
